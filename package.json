{
  "name": "graphql-api",
  "version": "0.0.1",
  "description": "Graphql api with node and mongoose",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node --presets env",
    "build": "babel server.js -d dist --presets env",
    "serve": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoineneff/graphql-api.git"
  },
  "keywords": [
    "graphql",
    "api",
    "mongoose"
  ],
  "author": "Antoine Neff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antoineneff/graphql-api/issues"
  },
  "homepage": "https://github.com/antoineneff/graphql-api#readme",
  "dependencies": {
    "express": "^4.15.3",
    "graphql": "^0.10.1",
    "mongoose": "^4.10.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "nodemon": "^1.11.0"
  }
}
